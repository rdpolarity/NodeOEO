{
  "name": "oeo-assets",
  "version": "0.0.0",
  "description": "Assets for tornament",
  "author": {
    "name": "rdpolarity",
    "email": "rdpolarity@gmail.com",
    "url": "https://aydie.me"
  },
  "scripts": {
    "build": "parcel build src/dashboard/*.html -d dashboard && parcel build src/graphics/*.html -d graphics",
    "dev": "concurrently \"parcel watch src/dashboard/*.html -d dashboard --public-url ./\" \"parcel watch src/graphics/*.html -d graphics --public-url ./\""
  },
  "files": [
    "dashboard",
    "graphics",
    "extension.js",
    "extension"
  ],
  "keywords": [
    "",
    "nodecg-bundle"
  ],
  "browserslist": "last 2 chrome versions",
  "nodecg": {
    "compatibleRange": "^1.1.1",
    "dashboardPanels": [
      {
        "name": "welcome",
        "title": "Welcome",
        "width": 3,
        "file": "welcome.html",
        "headerColor": "#EE9B51"
      },
      {
        "name": "scoreboard",
        "title": "Scoreboard",
        "width": 6,
        "file": "scoreboard.html",
        "headerColor": "#EE9B51"
      },
      {
        "name": "annoucement",
        "title": "Annoucement",
        "width": 3,
        "file": "annoucement.html",
        "headerColor": "#EE9B51"
      },
      {
        "name": "countdown",
        "title": "Countdown",
        "width": 3,
        "file": "countdown.html",
        "headerColor": "#EE9B51"
      },
      {
        "name": "teams",
        "title": "Teams",
        "width": 6,
        "file": "teams.html",
        "headerColor": "#EE9B51"
      },
      {
        "name": "livestream",
        "title": "Livestream",
        "width": 3,
        "file": "livestream.html",
        "headerColor": "#EE9B51"
      },
      {
        "name": "twitter",
        "title": "Twitter",
        "width": 3,
        "file": "twitter.html",
        "headerColor": "#EE9B51"
      }
    ],
    "graphics": [
      {
        "file": "gameplay.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "starting.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "vsing.html",
        "width": 1920,
        "height": 1080
      },
      {
        "file": "credits.html",
        "width": 1920,
        "height": 1080
      }
    ],
    "assetCategories": [
      {
        "name": "teamLogos",
        "title": "teamLogos",
        "allowedTypes": [
          "jpg",
          "jpeg",
          "gif",
          "png"
        ]
      },
      {
        "name": "sponsorLogos",
        "title": "sponsorLogos",
        "allowedTypes": [
          "jpg",
          "jpeg",
          "gif",
          "png"
        ]
      }
    ]
  },
  "repository": "https://github.com/nodecg/nodecg.git",
  "license": "Apache-2.0",
  "devDependencies": {
    "cssnano": "^4.1.10",
    "sass": "^1.22.9"
  },
  "dependencies": {
    "@material-ui/core": "^4.3.1",
    "animejs": "^3.1.0",
    "concurrently": "^4.1.1",
    "react": "^16.8.6",
    "react-anime": "^2.2.0",
    "react-countdown-now": "^2.1.1",
    "react-dom": "^16.8.6",
    "react-particles-js": "^2.7.0",
    "react-slick": "^0.25.2",
    "slick-carousel": "^1.8.1",
    "use-nodecg": "^0.4.0"
  }
